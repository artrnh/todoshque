{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "curly": [
      true,
      "ignore-same-line"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "jsx-boolean-value": false,
    "object-literal-sort-keys": false
  }
}