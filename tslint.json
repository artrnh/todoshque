{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "curly": [
      true,
      "ignore-same-line"
    ],
    "object-literal-sort-keys": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "case-insensitive"
      }
    ],
    "max-line-length": [
      true,
      256
    ],
    "member-access": [
      false
    ],
    "no-console": [
      false
    ],
    "no-unused-expression": false,
    "no-string-literal": false,
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "^_"
      }
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ]
  }
}